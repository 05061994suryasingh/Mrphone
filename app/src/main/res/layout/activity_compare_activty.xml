<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_compare_activty"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.chromeinfotech.mrphone.view.ui.activity.compare.CompareActivty">
    <include layout="@layout/layout_search_phone_toolbar"
        android:id="@+id/toolbar_activity_compare">
    </include>

    <android.support.v7.widget.RecyclerView
        android:layout_below="@+id/toolbar_activity_compare"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/dim_5"
        android:id="@+id/recycleview_compare_phone"
        android:layout_height="match_parent">
    </android.support.v7.widget.RecyclerView>
</RelativeLayout>

<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_phone_finder"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.chromeinfotech.mrphone.view.ui.activity.phonefinder.PhoneFinderActivity">
    <include layout="@layout/phone_finder_toolbar"
        android:id="@+id/phoneFinderToolbar">

    </include>
    <android.support.v7.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_below="@+id/phoneFinderToolbar"
        android:id="@+id/recyclerviewPhonefinder"
        android:layout_height="match_parent">
    </android.support.v7.widget.RecyclerView>
    <android.support.design.widget.FloatingActionButton
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/tick"
        android:background="@drawable/floatbackground"
        android:layout_marginBottom="@dimen/dim_10"
        android:layout_marginRight="@dimen/dim_10"
        app:backgroundTint="@color/yellow"
        android:elevation="0dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        app:borderWidth="0dp"
        app:elevation="6dp"
        app:fabSize="normal" />
</RelativeLayout>
